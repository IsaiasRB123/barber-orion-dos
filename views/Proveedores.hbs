            <input type="tel" id="telefono" name="telefono" required title="Por favor, ingresa solo números">
            <div id="telefonoError" class="error-message"></div><br>

            <!-- Campo NIT generado automáticamente -->
            <label for="nit">NIT:</label>
            <input type="text" id="nit" name="nit" readonly>

            <button type="submit" class="register-btn">Registrar</button>
          </form>
        </div>
